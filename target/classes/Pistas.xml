<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="ModelosPrueba.Pistas">
        <table name="pistas" schema="crimeBook" catalog=""/>
        <id-class class="ModelosPrueba.PistasPK"/>
        <attributes>
            <id name="idPrueba">
                <column name="idPrueba" precision="-1"/>
            </id>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="texto">
                <column name="texto" length="1000"/>
            </basic>
            <basic name="tiempo">
                <column name="tiempo" nullable="false" precision="-1"/>
            </basic>
            <basic name="intentos">
                <column name="intentos" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
