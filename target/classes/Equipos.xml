<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="ModelosPrueba.Equipos">
        <table name="equipos" schema="crimeBook" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="codigo">
                <column name="codigo" precision="-1"/>
            </basic>
            <basic name="nombre">
                <column name="nombre" length="50"/>
            </basic>
            <basic name="tiempo">
                <column name="tiempo" precision="-1"/>
            </basic>
            <basic name="idPartida">
                <column name="idPartida" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
