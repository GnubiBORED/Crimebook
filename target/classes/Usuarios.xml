<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="ModelosPrueba.Usuarios">
        <table name="usuarios" schema="crimeBook" catalog=""/>
        <attributes>
            <id name="username">
                <column name="username" length="15"/>
            </id>
            <basic name="email">
                <column name="email" length="50"/>
            </basic>
            <basic name="nombre">
                <column name="nombre" length="50"/>
            </basic>
            <basic name="apellidos">
                <column name="apellidos" length="50"/>
            </basic>
            <basic name="contrasenya">
                <column name="contrasenya" length="30"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
